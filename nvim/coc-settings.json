{
    "codeLens.enable": true,
    "java.configuration.runtimes": [
        {
            "default": true,
            "name": "JavaSE-1.8",
            "path": "/Library/Java/JavaVirtualMachines/jdk1.8.0_301.jdk/Contents/Home"
        },
        {
            "name": "JavaSE-11",
            "path": "/Library/Java/JavaVirtualMachines/openjdk-11.jdk/Contents/Home"
        }
    ],
    "java.home": "/Library/Java/JavaVirtualMachines/adoptopenjdk-15.jdk/Contents/Home",
    "java.referencesCodeLens.enabled": true,
    "java.trace.server": "verbose",
    "git.enableGutters": true,
    "languageserver": {
        "nginx-language-server": {
            "nginx.enable": true,
            "command": "nginx-language-server",
            "filetypes": [
                "nginx"
            ],
            "rootPatterns": [
                "nginx.conf",
                ".git"
            ]
        }
    },
    "java.format.settings.url": "/Users/beke/.config/coc/extensions/node_modules/redhat.java/eclipse-formatter.xml",
    "explorer.root.strategies": [
        "workspace",
        "cwd",
        "sourceBuffer",
        "reveal"
    ],
    "explorer.presets": {
        "code": {
            "root-uri": "~/Code",
            "quit-on-open": true
        },
        "floating": {
            "position": "floating",
            "floating-position": "center",
            "quit-on-open": true
        },
        "leftSide":{
            "position": "left",
            "quit-on-open": false
        }
    },
    "explorer.file.showHiddenFiles": true,
    "explorer.icon.enableNerdfont": true,
    "explorer.width": 30,
    "sql.database": "mysql",
    "php-cs-fixer.enable": true,
    "php-cs-fixer.toolPath": "/usr/local/opt/php-cs-fixer/bin/php-cs-fixer",
    "php-cs-fixer.config": "/Users/beke/.php-cs-fixer.php",
    "php-cs-fixer.rules": "@PSR12,@Symfony",
    "phpactor.enable": true,
    "phpactor.path": "/Users/beke/.vim/plugged/phpactor/bin/phpactor"
}
